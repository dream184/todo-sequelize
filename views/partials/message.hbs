{{#if warning_msg}}
<div class="alert alert-warning" role="alert">
  {{warning_msg}}
</div>
{{/if}}

{{#if login_failure_msg}}
<div class="alert alert-danger" role="alert">
  {{login_failure_msg}}
</div>
{{/if}}

{{#if logout_msg}}
<div class="alert alert-success" role="alert">
  {{logout_msg}}
</div>
{{/if}}

{{#if errors}}
{{#each errors}}
<div class="alert alert-danger" role="alert">
  {{this.message}}
</div>
{{/each}}
{{/if}}
